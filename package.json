{
  "name": "valuation-service",
  "version": "0.0.1",
  "description": "Microservice for real estate valuation and liquidity scoring",
  "author": "Vector Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:valuation": "nest build valuation",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/valuation/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d ./db/datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./db/datasource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./db/datasource.ts",
    "parse-geo-osm": "ts-node -r tsconfig-paths/register ./scripts/parse-geo-osm.ts",
    "parse-streets-osm": "ts-node -r tsconfig-paths/register ./scripts/parse-streets-osm.ts",
    "sync-from-aggregator": "ts-node -r tsconfig-paths/register ./scripts/sync-from-aggregator-db.ts",
    "import-aggregator-csv": "ts-node -r tsconfig-paths/register ./scripts/import-aggregator-csv.ts",
    "process-aggregator-dump": "ts-node -r tsconfig-paths/register ./scripts/process-aggregator-dump.ts",
    "import-polygons": "ts-node -r tsconfig-paths/register ./scripts/import-polygons-osm.ts"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.5.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "vector-shared": "^1.1.466"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/models(|/.*)$": "<rootDir>/libs/models/src/$1",
      "^@libs/rabbitmq(|/.*)$": "<rootDir>/libs/rabbitmq/src/$1"
    }
  }
}
